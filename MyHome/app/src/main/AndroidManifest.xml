<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dignityhealth.myhome">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.MyHomeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".features.home.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".features.fad.FadActivity"
            android:label="@string/app_name" />

        <activity
            android:name=".features.profile.ProfileActivity"
            android:label="@string/app_name" />

        <activity
            android:name=".features.settings.SettingsActivity"
            android:label="@string/app_name" />

        <activity
            android:name=".features.contact.ContactUsActivity"
            android:label="@string/app_name" />

    </application>

</manifest>