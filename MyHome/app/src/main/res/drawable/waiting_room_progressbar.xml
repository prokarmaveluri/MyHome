<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:angle="0"
                android:centerColor="@color/waiting_room_progress_center"
                android:endColor="@color/waiting_room_progress_right"
                android:startColor="@color/waiting_room_progress_left"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:angle="0"
                    android:centerColor="@color/waiting_room_progress_center"
                    android:endColor="@color/waiting_room_progress_left"
                    android:startColor="@color/waiting_room_progress_right"/>
            </shape>
        </clip>
    </item>
</layer-list>